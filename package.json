{
  "name": "nim-pools-hub-miner",
  "version": "0.2.5",
  "author": "Albermonte <alberto.monterroso@hotmail.com>",
  "description": "Nimiq GUI Miner",
  "homepage": "https://github.com/Albermonte/nim-pools-hub-miner",
  "license": "GPLv3",
  "main": "./dist/electron/main.js",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "build": "node .electron-vue/build.js && electron-builder",
    "deploy": "node .electron-vue/build.js && electron-builder --publish always",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "node postinstall",
    "release": "release-it"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "@nimiq/core": "^1.5.8",
    "@nimiq/style": "^0.8.3",
    "@nimiq/utils": "^0.9.0",
    "@sentry/electron": "^4.2.0",
    "axios": "^0.24.0",
    "bindings": "https://github.com/Albermonte/node-bindings",
    "chart.js": "^2.9.4",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "node-machine-id": "^1.1.12",
    "node-powershell": "^4.0.0",
    "systeminformation": "^5.17.3",
    "vue": "^2.6.14",
    "vue-chartjs": "^3.5.1",
    "vue-router": "^3.5.3",
    "vue-slider-component": "^3.2.24",
    "vuescroll": "^4.17.3",
    "vuex": "^3.6.2",
    "vuex-electron": "^1.0.3",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@sentry/webpack-plugin": "^1.20.0",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "babel-loader": "^8.2.3",
    "cfonts": "^2.10.0",
    "chalk": "^4.1.2",
