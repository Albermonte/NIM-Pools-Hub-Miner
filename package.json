{
  "name": "nim-pools-hub-miner",
  "version": "0.1.3",
  "author": "Albermonte <alberto.monterroso@hotmail.com>",
  "description": "Nimiq GUI Miner",
  "homepage": "https://github.com/Albermonte/nim-pools-hub-miner",
  "license": "GPLv3",
  "main": "./dist/electron/main.js",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "build": "node .electron-vue/build.js && electron-builder",
    "release": "node .electron-vue/build.js && electron-builder --publish always",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "node postinstall"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.4",
    "@nimiq/core": "^1.5.5",
    "@nimiq/style": "^0.8.2",
    "@nimiq/utils": "^0.5.1",
    "axios": "^0.19.2",
    "bindings": "https://github.com/Albermonte/node-bindings",
    "chart.js": "^2.9.3",
    "electron-log": "^4.2.2",
    "electron-store": "^5.2.0",
    "electron-updater": "^4.3.1",
    "node-powershell": "^4.0.0",
    "systeminformation": "^4.26.10",
    "vue": "^2.6.11",
    "vue-chartjs": "^3.5.0",
    "vue-router": "^3.3.4",
    "vue-slider-component": "^3.2.2",
    "vuescroll": "^4.16.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@vue/cli-plugin-typescript": "^4.4.6",
    "babel-loader": "^8.1.0",
    "cfonts": "^2.8.5",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "del": "^5.1.0",
    "devtron": "^1.4.0",
    "electron": "^5.0.13",
    "electron-builder": "^22.7.0",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-rebuild": "^1.11.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "0.9.0",
    "multispinner": "^0.2.1",
    "node-lmdb": "https://github.com/tomkha/node-lmdb#win_delay_load_hook",
    "node-loader": "^1.0.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^9.0.1",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.6",
    "url-loader": "^4.1.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  },
  "optionalDependencies": {
    "osx-temperature-sensor": "1.0.7"
  },
  "resolutions": {
